{
    "name": "MiShare",
    "version": "0.1",
    "manifest_version": 2,
    "description": "__MSG_extension_description__",
    "default_locale": "en",
    "icons": {
        "96": "icons/96.png",
        "128": "icons/128.png",
        "256": "icons/256.png"
    },
    "browser_action": {
        "default_title": "",
        "default_icon": "icons/128.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "storage"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "web_accessible_resources": [
        "fonts/notosans.otf"
    ]
}